        let greet = x => console.log(x);
        greet("Hello");


        let age = 19;
        let welcome = (age<18)? ()=> console.log("Hii"): ()=> console.log("Hello");

        welcome()


        console.log("Hey")
        setTimeout(()=>{
            console.log("This will take time")
        },3000);

        console.log("This doesn't take time")


        let myPromise = new Promise((resolve , reject)=>{
            let success = true;
            if(success){
                resolve("task completed");

            }
            else{
                reject("task rejected");
            }
        })
    </script>
    
